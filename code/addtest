#! /usr/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1f3e700 .scope module, "main" "main" 2 1;
 .timescale 0 0;
v0x1f772d0_0 .var "a", 9 0;
v0x1f77370_0 .var "b", 9 0;
v0x1f77420_0 .net "c", 9 0, v0x1f76f60_0; 1 drivers
v0x1f774d0 .array "data", 12 0, 9 0;
v0x1f77580_0 .var/i "i", 31 0;
S_0x1f3e400 .scope module, "add1" "adder" 2 8, 3 1, S_0x1f3e700;
 .timescale 0 0;
v0x1f3eec0_0 .net "a", 9 0, v0x1f772d0_0; 1 drivers
v0x1f76ec0_0 .net "b", 9 0, v0x1f77370_0; 1 drivers
v0x1f76f60_0 .var "c", 9 0;
v0x1f77000_0 .net "signa", 0 0, L_0x1f77810; 1 drivers
v0x1f770b0_0 .net "signb", 0 0, L_0x1f778b0; 1 drivers
v0x1f77150_0 .net "vala", 8 0, L_0x1f77600; 1 drivers
v0x1f77230_0 .net "valb", 8 0, L_0x1f77720; 1 drivers
E_0x1f511a0 .event edge, v0x1f77000_0, v0x1f770b0_0, v0x1f77150_0, v0x1f77230_0;
L_0x1f77600 .part v0x1f772d0_0, 0, 9;
L_0x1f77720 .part v0x1f77370_0, 0, 9;
L_0x1f77810 .part v0x1f772d0_0, 9, 1;
L_0x1f778b0 .part v0x1f77370_0, 9, 1;
    .scope S_0x1f3e400;
T_0 ;
    %wait E_0x1f511a0;
    %load/v 8, v0x1f77000_0, 1;
    %load/v 9, v0x1f770b0_0, 1;
    %cmp/u 8, 9, 1;
    %jmp/0xz  T_0.0, 4;
    %load/v 8, v0x1f77150_0, 9;
    %mov 17, 0, 1;
    %load/v 18, v0x1f77230_0, 9;
    %mov 27, 0, 1;
    %add 8, 18, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v0x1f76f60_0, 0, 8;
    %load/v 8, v0x1f770b0_0, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 9, 0;
    %assign/v0/x1 v0x1f76f60_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x1f77230_0, 9;
    %load/v 17, v0x1f77150_0, 9;
    %cmp/u 8, 17, 9;
    %jmp/0xz  T_0.2, 5;
    %load/v 8, v0x1f77150_0, 9;
    %mov 17, 0, 1;
    %load/v 18, v0x1f77230_0, 9;
    %mov 27, 0, 1;
    %sub 8, 18, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v0x1f76f60_0, 0, 8;
    %load/v 8, v0x1f77000_0, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 9, 0;
    %assign/v0/x1 v0x1f76f60_0, 0, 8;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v0x1f77230_0, 9;
    %mov 17, 0, 1;
    %load/v 18, v0x1f77150_0, 9;
    %mov 27, 0, 1;
    %sub 8, 18, 10;
    %ix/load 0, 10, 0;
    %assign/v0 v0x1f76f60_0, 0, 8;
    %load/v 8, v0x1f770b0_0, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 9, 0;
    %assign/v0/x1 v0x1f76f60_0, 0, 8;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1f3e700;
T_1 ;
    %vpi_call 2 13 "$readmemb", "adder.bin", v0x1f774d0;
    %set/v v0x1f77580_0, 0, 32;
T_1.0 ;
    %load/v 8, v0x1f77580_0, 32;
   %cmpi/s 8, 10, 32;
    %jmp/0xz T_1.1, 5;
    %ix/getv/s 3, v0x1f77580_0;
    %load/av 8, v0x1f774d0, 10;
    %set/v v0x1f772d0_0, 8, 10;
    %ix/load 0, 1, 0;
    %load/vp0/s 18, v0x1f77580_0, 32;
    %ix/get/s 3, 18, 32;
    %load/av 8, v0x1f774d0, 10;
    %set/v v0x1f77370_0, 8, 10;
    %delay 100, 0;
    %vpi_call 2 21 "$display", "%0d:%0d, %0d:%0d, %0d:%0d", &PV<v0x1f772d0_0, 9, 1>, &PV<v0x1f772d0_0, 0, 9>, &PV<v0x1f77370_0, 9, 1>, &PV<v0x1f77370_0, 0, 9>, &PV<v0x1f77420_0, 9, 1>, &PV<v0x1f77420_0, 0, 9>;
    %ix/load 0, 2, 0;
    %load/vp0/s 8, v0x1f77580_0, 32;
    %set/v v0x1f77580_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 24 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
